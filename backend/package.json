{
  "name": "neosale-backend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx app.ts",
    "back": "npm run dev",
    "build": "tsc",
    "start": "node dist/app.js",
    "prisma:generate": "prisma generate --schema=../prisma/schema.prisma",
    "prisma:pull": "prisma db pull --schema=../prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=../prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=../prisma/schema.prisma --env-file=./.env"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "prisma": "^6.10.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
