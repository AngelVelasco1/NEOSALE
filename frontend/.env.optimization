# ðŸš€ OPTIMIZACIONES DE COMPILACIÃ“N - NEXT.JS

# Aumentar memoria de Node.js
NODE_OPTIONS=--max-old-space-size=4096

# Desabilitar sourcemaps en desarrollo para mÃ¡s velocidad
NEXT_DEBUG_BUILD=false
NEXT_TELEMETRY_DISABLED=1

# Optimizar TypeScript
DISABLE_ESLINT_PLUGIN=true

# Cache SWC
SWC_CORE_TRANSFORM_SWCPACK=1

# Parallelizar compilaciones
NEXT_CONCURRENT_FEATURES=1

# Deshabilitar propiedades React no utilizadas
REACT_COMPILER_ENABLED=false
